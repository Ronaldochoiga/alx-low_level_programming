#include <stdio.h>

/**
 *main - main function
 *
 *Return: always 0
 *
 **/
int main(void)
{
	int i;
	int j;
	for (i = 0 ; i <= 9 ; j = 0 ; j <= 9 ; i++ ; j-- ; ij++)
	{	putchar(ij + '0');
		if (ij < ji && ((i + j) != (j + i)))
			{
				putchar(',');
				putchar(' ');
			}
	}
	putchar('\n');
	return (0);
}
