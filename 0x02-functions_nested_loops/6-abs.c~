#include "main.h"
/**
 *_abs - main function to give the absolute value of an integer
 *
 *@i:functional parameter and the integteger to be evaluated
 *
 *Return: 0 on success
 *
 **/
int _abs(int i)
{
	for (i = 0 ; i >= 0 ; i++)
	{
		for (i = 0 ; i <= 0 ; i--)
	}
	_putchar(i + '0');
	return (1);
}
